{
  "name": "codegenapp",
  "version": "0.2.0",
  "description": "Autorest Azure code generator",
  "main": "dist/src/app/index.js",
  "scripts": {
    "start": "node dist/src/codegenApp.js",
    "run": "node dist/src/codegenApp.js",
    "debug": "node --max_old_space_size=4096 --inspect-brk dist/src/codegenApp.js",
    "test": "jest --forceExit --coverage",
    "lint-fix": "eslint . --fix --ext .ts --ignore-pattern *.d.ts --max-warnings=0",
    "lint": "eslint . --ext .ts --ignore-pattern *.d.ts --max-warnings=0",
    "eslint": "eslint --ext .ts test/",
    "coverage": "nyc npm run test",
    "eslint-fix": "eslint  . --fix --ext .ts",
    "build": "rimraf dist && tsc -p .",
    "start-testserver": "./node_modules/.bin/start-autorest-testserver",
    "stop-testserver": "./node_modules/.bin/stop-autorest-testserver",
    "watch": "tsc -p . --watch",
    "prepare": "npm run build",
    "clean": "ver > nul 2>&1 nul && npm run --silent clean-cmd || npm run --silent clean-bash",
    "clean-cmd": "if exist dist rmdir /s /q dist && exit 0 || exit 0 ",
    "clean-bash": "rm nul && rm -rf dist && exit 0 || exit 0"
  },
  "bin": {
    "publish-codegen-pipeline-result": "./dist/src/tools/publishCodegenPipelineResult.js"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/azure-sdk-pipeline.git"
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^2.0.4",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "chai": "^4.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "^27.3.1",
    "mocha": "^8.3.2",
    "mocha-prepare": "^0.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "ts-jest": "^26.4.4"
  },
  "dependencies": {
    "@azure/identity": "^2.0.0",
    "@azure/keyvault-secrets": "^4.1.0",
    "@azure/swagger-validation-common": "^0.1.2",
    "@octokit/core": "^3.1.4",
    "@octokit/rest": "^18.0.3",
    "async-retry": "^1.3.1",
    "azure-devops-node-api": "^10.2.1",
    "body-parser": "^1.19.0",
    "bufferutil": "^4.0.3",
    "canvas": "^2.7.0",
    "cluster": "^0.7.7",
    "convict": "^6.2.0",
    "csv-parse": "^4.15.4",
    "csv-parser": "^3.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "flatted": "^3.2.2",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.2",
    "hashcode": "^1.0.3",
    "hot-shots": "^8.5.2",
    "inversify": "^5.0.5",
    "inversify-express-utils": "^6.3.2",
    "js-yaml": "^3.14.1",
    "lodash": "^4.17.21",
    "memory-fs": "^0.5.0",
    "mongodb": "^3.6.10",
    "morgan": "^1.10.0",
    "mssql": "^6.3.1",
    "net": "^1.0.2",
    "node": "^12.11.0",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "node-yaml": "^3.2.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "pluralize": "^8.0.0",
    "properties-reader": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "serialize-error": "^8.1.0",
    "stack-typescript": "1.0.4",
    "ts-node": "^8.10.2",
    "typeorm": "^0.2.37",
    "typescript": "^4.1.3",
    "utf-8-validate": "^5.0.4",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.2"
  }
}
