{
  "name": "codegenapp",
  "version": "0.1.2",
  "description": "Autorest Azure code generator",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "run": "node dist/app.js",
    "debug": "node --max_old_space_size=4096 --inspect-brk ./dist/app.js",
    "test": "jest --forceExit --coverage",
    "eslint": "eslint --ext .ts test/",
    "coverage": "nyc npm run test",
    "eslint-fix": "eslint  . --fix --ext .ts",
    "build": "tsc -p .",
    "start-testserver": "./node_modules/.bin/start-autorest-testserver",
    "stop-testserver": "./node_modules/.bin/stop-autorest-testserver",
    "watch": "tsc -p . --watch",
    "prepare": "npm run build",
    "clean": "ver > nul 2>&1 nul && npm run --silent clean-cmd || npm run --silent clean-bash",
    "clean-cmd": "if exist dist rmdir /s /q dist && exit 0 || exit 0 ",
    "clean-bash": "rm nul && rm -rf dist && exit 0 || exit 0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://DepthCoverage@dev.azure.com/DepthCoverage/depthCoverage/_git/depthCoverage"
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^7.10.9",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "mocha-prepare": "^0.1.0",
    "nyc": "^14.1.1",
    "shelljs": "^0.8.3",
    "ts-jest": "^26.4.4"
  },
  "dependencies": {
    "@octokit/core": "^3.1.4",
    "@octokit/rest": "^18.0.3",
    "azure-devops-node-api": "^10.2.1",
    "csv-parser": "^3.0.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.2",
    "hashcode": "^1.0.3",
    "js-yaml": "^3.14.1",
    "net": "^1.0.2",
    "node": "^12.11.0",
    "node-yaml": "^3.2.0",
    "path": "^0.12.7",
    "mssql": "^6.3.1",
    "pluralize": "^8.0.0",
    "stack-typescript": "1.0.4",
    "ts-node": "^8.5.4",
    "typescript": "^4.1.3"
  }
}
