{
  "name": "publish-sdkcodgen-pipeline-result",
  "version": "0.1.4",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "start": "node dist/Logger/index.js",
    "run": "node dist/Logger/index.js",
    "debug": "node --max_old_space_size=4096 --inspect-brk dist/src/Logger/index.js",
    "test": "jest --forceExit --coverage",
    "lint": " prettier --check \"**/*.ts\"",
    "eslint": "eslint --ext .ts test/",
    "coverage": "nyc npm run test",
    "lint-fix": "prettier --write \"**/*.ts\"",
    "eslint-fix": "eslint  . --fix --ext .ts",
    "build": "tsc -p .",
    "start-testserver": "./node_modules/.bin/start-autorest-testserver",
    "stop-testserver": "./node_modules/.bin/stop-autorest-testserver",
    "watch": "tsc -p . --watch",
    "prepare": "npm run build",
    "clean": "ver > nul 2>&1 nul && npm run --silent clean-cmd || npm run --silent clean-bash",
    "clean-cmd": "if exist dist rmdir /s /q dist && exit 0 || exit 0 ",
    "clean-bash": "rm nul && rm -rf dist && exit 0 || exit 0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://DepthCoverage@dev.azure.com/DepthCoverage/depthCoverage/_git/depthCoverage"
  },
  "dependencies": {
    "@azure/swagger-validation-common": "^0.1.2",
    "line-reader": "^0.4.0",
    "js-yaml": "^3.14.1",
    "node-yaml": "^3.2.0"
  }
}
